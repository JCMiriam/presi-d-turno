{
  "name": "presi-d-turno",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server",
    "shared"
  ],
  "scripts": {
    "dev": "concurrently -n server,client \"npm run dev -w server\" \"npm run dev -w client\"",
    "build": "npm run build -w shared && npm run build -w server && npm run build -w client",
    "start": "npm run start -w server",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.cjs,.mjs,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.cjs,.mjs,.ts,.vue --fix",
    "fix:all": "npm run format && npm run lint:fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2"
  }
}
